<RelativeLayout 
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@drawable/red"
     >
<GridView
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:id="@+id/grid_about"
    android:layout_marginRight="20dp"
    android:layout_marginLeft="20dp"
    android:layout_marginTop="20dp"
    android:layout_marginBottom="20dp"
    android:columnWidth="64dp"
        android:gravity="center"
        android:horizontalSpacing="10dp"
        android:numColumns="2"
        android:stretchMode="columnWidth"
        android:verticalSpacing="10dp"
    android:background="#BFc4e4f5">
	
   

</GridView>
<RelativeLayout 
     android:layout_width="match_parent"
     android:layout_height="wrap_content"
     android:layout_marginRight="20dp"
     android:layout_marginLeft="20dp"
     android:padding="20dp"
     android:layout_below="@+id/grid_about"
     android:background="#BFc4e4f5">
 <TextView 
     android:id="@+id/text_about"
     android:layout_width="match_parent"
     android:layout_height="wrap_content"
     android:text="Â© Saratoga High Application Devlopers Club (2012-2014) "/>

      <TextView 
     android:id="@+id/text_about_developertitle"
	android:layout_below="@+id/text_about"
     android:layout_width="wrap_content"
     android:layout_height="wrap_content"
     android:text="Developer Acknowledgments:"
     android:textStyle="bold"/>
  <TextView 
     android:layout_below="@+id/text_about_developertitle"
     android:id="@+id/text_about_developer"

     android:layout_width="wrap_content"
     android:layout_height="wrap_content"
     android:text=" Aditya Aggarwal and Eugene Che"/>
 </RelativeLayout>
</RelativeLayout>
